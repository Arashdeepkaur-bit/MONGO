<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Data</title>
    <style>
        table {
            
            width: 100%;
        }
        th, td {
            border: 2px solid #ddd;
            padding: 8px;
        }
        th {
            background-color: #f4f4f4;
            text-align: left;
        }
    </style>
</head>
<body>
    <h1>User Data</h1>
    <table>
        <thead>
            <tr>
                <th> UserID</th>
                <th>Name</th>
                <th>password</th>
             
            </tr>
        </thead>
        <tbody>
            <% para.forEach(user => { %>
                <tr>
                    <td><%= user.id %></td>
                    <td><%= user.name %></td>
                    <td><%= user.passward %></td>
                    
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>